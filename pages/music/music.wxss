.banner_view {
  height: 260rpx;
  display: block;
}
.banner_img {
  height:260rpx;
  width:100%;
}
.list-content {
   width: 100%;
   height: auto;
   overflow: hidden;
}
.listbar {
   width: 100%;
   height: 120rpx;
   display: flex;
   align-items: center;
   border-bottom: 1px solid #ccc;
}
[class^="tel"] {
   height: 80%;
   float: left;
   box-sizing: border-box;
}
.tel_num , .tel-icon {
   display: flex;
   align-items: center;
}
.tel_num {
   width: 30%;
   justify-content: center;
}
.tel_name {
   width: 50%;
}
.tel-icon {
   width: 20%;
}
.Img-Music {
   width: 80rpx;
   height: 80rpx;
   display: inline-block;
}
.tel_name .txt_bag {
   font-size: 30rpx;
}
.tel_name .txt_small {
   font-size: 24rpx;
}