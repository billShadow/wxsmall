
.listbar {
  width:100%;
  height: 60px;
  border: 1px solid red;
  line-height: 60px;
  font-size: 12px
}
.tel_num {
  width:16%;
  display: block;
  float:left;
  color:#999;
  text-align: center;
  line-height: 60px;
}
.tel_name {
  width:50%;
  display: block;
  float:left;
  line-height: 30px;
  text-align: left;
}
.tel_name .tel_bag {
  font-size: 14px;
  color:black;
  padding: 8px 0 0 0;
}
.tel_name .tel_small {
  font-size: 12px;
  color:#999;
  padding: -10px 0 0 0 ;
}
.tel_act {
  width:20%;
  display: block;
  float:left;
}